  <div class="container">
    <div class="row">
      <% @orders.each do |order| %>
        <div class="col-2  p-1">
          <div class="card" style='height: 180px;'>
            <div class="card-body d-flex flex-column justify-content-between">
                <h5 class='card-title'><%= order.super_power.name %></h5>
                <p class='card-text'><strong>$<%= order.super_power.price %></strong></p>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
